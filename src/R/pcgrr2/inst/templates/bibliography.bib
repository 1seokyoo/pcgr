@ARTICLE{Ying2010-jc,
  title       = "Evidence that localized variation in primate sequence
                 divergence arises from an influence of nucleosome placement on
                 {DNA} repair",
  author      = "Ying, Hua and Epps, Julian and Williams, Rohan and Huttley,
