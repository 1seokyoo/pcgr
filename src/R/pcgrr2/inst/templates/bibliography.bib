  journal     = "Am. J. Hum. Genet.",
  volume      =  82,
  number      =  1,
  pages       = "100--112",
  year        =  2008,
  keywords    = "2007-2010"
}

@ARTICLE{Chen2008-di,
  title       = "{FitSNPs}: highly differentially expressed genes are more
                 likely to have variants associated with disease",
  author      = "Chen, Rong and Morgan, Alex A and Dudley, Joel and Deshpande,
                 Tarangini and Li, Li and Kodama, Keiichi and Chiang, Annie P
                 and Butte, Atul J",
