@ARTICLE{Casto2011-ag,
  title       = "Genome-wide association study {SNPs} in the human genome
                 diversity project populations: does selection affect unlinked
                 {SNPs} with shared trait associations?",
  author      = "Casto, Amanda M and Feldman, Marcus W",
  affiliation = "Department of Genetics, Stanford University, Stanford,
