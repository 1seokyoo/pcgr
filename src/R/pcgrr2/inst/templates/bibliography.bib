@ARTICLE{Khatri2012-ag,
  title       = "Ten years of pathway analysis: current approaches and
                 outstanding challenges",
  author      = "Khatri, Purvesh and Sirota, Marina and Butte, Atul J",
  affiliation = "Division of Systems Medicine, Department of Pediatrics,
