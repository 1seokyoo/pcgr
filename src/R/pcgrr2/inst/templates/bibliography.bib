@ARTICLE{Korona2010-fg,
  title       = "The high fidelity and unique error signature of human {DNA}
                 polymerase varepsilon",
  author      = "Korona, Dagmara A and Lecompte, Kimberly G and Pursell,
                 Zachary F",
  affiliation = "Department of Biochemistry and Tulane Cancer Center, Tulane
