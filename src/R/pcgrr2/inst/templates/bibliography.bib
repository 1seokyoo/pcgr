@ARTICLE{Chen2011-kv,
  title       = "Replication-associated mutational asymmetry in the human
                 genome",
  author      = "Chen, Chun-Long and Duquenne, Lauranne and Audit, Benjamin and
                 Guilbaud, Guillaume and Rappailles, Aur\'{e}lien and Baker,
                 Antoine and Huvet, Maxime and d'Aubenton-Carafa, Yves and
