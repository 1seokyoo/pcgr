@ARTICLE{Zhu2004-dx,
  title       = "An evolutionary perspective on single-nucleotide polymorphism
                 screening in molecular cancer epidemiology",
  author      = "Zhu, Yong and Spitz, Margaret R and Amos, Christopher I and
