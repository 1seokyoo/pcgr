@ARTICLE{Frijters2010-ec,
  title       = "Literature mining for the discovery of hidden connections
                 between drugs, genes and diseases",
  author      = "Frijters, Raoul and van Vugt, Marianne and Smeets, Ruben and
                 van Schaik, Ren\'{e} and de Vlieg, Jacob and Alkema, Wynand",
  affiliation = "Computational Drug Discovery (CDD), Nijmegen Centre for
