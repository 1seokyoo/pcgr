@ARTICLE{Timsit1999-ac,
  title       = "{DNA} structure and polymerase fidelity",
  author      = "Timsit, Y",
  affiliation = "Institut de Biologie Physico-Chimique, CNRS - UPR 9080, 13,
                 rue Pierre et Marie Curie, Paris, 75005, France.
                 timsit@ibpc.fr",
  abstract    = "The accuracy of DNA replication results from both the
