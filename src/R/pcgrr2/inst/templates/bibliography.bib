@ARTICLE{Lyon2012-uu,
  title       = "Identifying disease mutations in genomic medicine settings:
                 current challenges and how to accelerate progress",
  author      = "Lyon, Gholson J and Wang, Kai",
  affiliation = "Cold Spring Harbor Laboratory, New York, NY 11797, USA.
                 GholsonJLyon@gmail.com.",
  abstract    = "ABSTRACT: The pace of exome and genome sequencing is
