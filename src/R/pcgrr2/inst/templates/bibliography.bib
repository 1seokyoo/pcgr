@ARTICLE{Marigorta2011-rd,
  title       = "Recent human evolution has shaped geographical differences in
                 susceptibility to disease",
  author      = "Marigorta, Urko M and Lao, Oscar and Casals, Ferran and
                 Calafell, Francesc and Morcillo-Su\'{a}rez, Carlos and Faria,
