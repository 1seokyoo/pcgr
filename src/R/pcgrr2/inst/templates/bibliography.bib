@ARTICLE{Chen2013-qy,
  title       = "Patterns and Mutational Signatures of Tandem Base
                 Substitutions Causing Human Inherited Disease",
  author      = "Chen, Jian-Min and F\'{e}rec, Claude and Cooper, David N",
