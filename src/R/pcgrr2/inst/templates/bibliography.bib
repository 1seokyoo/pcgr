@ARTICLE{Kunkel2000-nz,
  title       = "{DNA} replication fidelity",
  author      = "Kunkel, Thomas A and Bebenek, Katarzyna",
  affiliation = "Laboratory of Molecular Genetics and Laboratory of Structural
                 Biology, National Institute of Environmental Health Sciences,
                 Research Triangle Park, North Carolina 27709, USA.
                 kunkel@niehs.nih.gov",
  abstract    = "DNA replication fidelity is a key determinant of genome
                 stability and is central to the evolution of species and to
