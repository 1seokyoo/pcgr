  language    = "en"
}

@ARTICLE{Alexandrov2015-ib,
  title       = "A mutational signature in gastric cancer suggests therapeutic
                 strategies",
  author      = "Alexandrov, Ludmil B and Nik-Zainal, Serena and Siu, Hoi
                 Cheong and Leung, Suet Yi and Stratton, Michael R",
  affiliation = "Cancer Genome Project, Wellcome Trust Sanger Institute,
                 Hinxton, Cambridgeshire CB10 1SA, UK. Theoretical Biology and
                 Biophysics (T-6), Los Alamos National Laboratory, Los Alamos,
                 New Mexico 87545, USA. Center for Nonlinear Studies, Los
                 Alamos National Laboratory, Los Alamos, New Mexico 87545, USA.
