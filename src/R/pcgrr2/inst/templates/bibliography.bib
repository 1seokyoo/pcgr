@ARTICLE{Supek2015-eb,
  title       = "Differential {DNA} mismatch repair underlies mutation rate
                 variation across the human genome",
  author      = "Supek, Fran and Lehner, Ben",
  affiliation = "1] EMBL-CRG Systems Biology Unit, Centre for Genomic
