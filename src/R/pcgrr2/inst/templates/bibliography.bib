@ARTICLE{Stoletzki2011-ws,
  title       = "The positive correlation between {dN/dS} and dS in mammals is
                 due to runs of adjacent substitutions",
  author      = "Stoletzki, Nina and Eyre-Walker, Adam",
  affiliation = "Centre for Study of Evolution, School of Life Sciences,
